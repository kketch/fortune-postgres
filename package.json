{
  "name": "fortune-postgres",
  "description": "Postgres adapter for Fortune.",
  "version": "1.6.10",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fortunejs/fortune-postgres"
  },
  "scripts": {
    "lint": "eslint lib test",
    "test": "npm run lint && node test",
    "postpublish": "git tag `npm v fortune-postgres version` && git push origin --tags"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "pg": "^8.4.1"
  },
  "optionalDependencies": {
    "pg-native": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-boss": "^1.0.6",
    "fortune": "^5.5.14",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2",
    "tapdance": "^5.1.1"
  },
  "files": [
    "lib/",
    "LICENSE"
  ],
  "eslintConfig": {
    "extends": "boss",
    "rules": {
      "strict": 0,
      "indent": 0
    }
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "fortune",
    "postgres",
    "postgresql",
    "sql",
    "adapter"
  ]
}
